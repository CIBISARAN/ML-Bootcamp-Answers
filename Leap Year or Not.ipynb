   "source": [
    "print(\"This Program describes the Year is Leap or Not!\")\n",
    "y=int(input(\"Enter a year :\\n\"))\n",
    "\n",
    "if (y%4==0 and y%100!=0) or y%400==0 :\n",
    "    print(\" The Year {} is Leap!\".format(y))\n",
    "else: \n",
    "    print(\" The Year {} is Not Leap!\".format(y))"
   ]
