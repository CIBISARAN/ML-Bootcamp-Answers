   "source": [
    "print(\"This Program calculates the Factorial\")\n",
    "f=int(input(\"Enter the Number : \"))\n",
    "fac=1\n",
    "for i in range(1,f+1):\n",
    "    fac=fac*i\n",
    "    \n",
    "print(\"Factorial of {s} is : {t} \".format(s=f,t=fac))"
   ]
